<section id="create">
    <div class="d-flex w-100 flex-wrap">
        <div class="d-flex justify-content-center">
            <div id="canvas-tools-area" class="position-relative"></div>
        </div>
        <div class="col-md-12 cover-photo">
            <!-- Replace the URL below with the URL of the cover photo -->
            <img src="https://images.pexels.com/photos/956981/milky-way-starry-sky-night-sky-star-956981.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=1" alt="Cover Photo" class="img-fluid">
        </div>
        <div class="col-md-3 canvas-attribute ps-3" style="height: 656.2px;">
            <div class="profile-details">
                <div class="profile-picture">
                    <!-- Replace the URL below with the URL of the user's profile picture -->
                    <img src="path_to_sample_profile_picture.jpg" alt="Profile Picture" class="img-fluid">
                </div>
                <div class="profile-info">
                    <div class="profile-container">
                        <div class="profile-image">
                            <!-- Place profile image here -->
                        </div>
                        <div class="profile-details">
                            <h2>John Doe</h2>
                            <p>Email: johndoe@example.com</p>
                            <p>Location: New York</p>
                            <p>Role: Data Scientist</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-9 canvas-area d-flex align-items-center overflow-auto justify-content-center"
            id="canvas-editor">
            <div id="canvas-editor-area" class="p-4 d-flex align-items-center justify-content-center">
            </div>
        </div>
    </div>
</section>
